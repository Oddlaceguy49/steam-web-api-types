{
  "compilerOptions": {
    "outDir": "./dist",
    "declaration": true,
    "sourceMap": true,
    "declarationMap": true,
    "emitDeclarationOnly": false,
    "strict": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "module": "nodenext",
    "target": "esnext",
    "moduleResolution": "nodenext",
    "typeRoots": [
      "./node_modules/@types"
    ],
    "types": [
      "bun",
      "node"
    ],
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "rootDir": "./src/generated"
  },
  "include": [
    "src/generated"
  ],
  "exclude": [
    "node_modules",
    "dist"
  ]
}