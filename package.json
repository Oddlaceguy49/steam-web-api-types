{
  "name": "@oddlaceguy49/steam-web-api-types",
  "version": "0.1.6",
  "description": "A comprehensive set of TypeScript type definitions for the raw Steam Web API.",
  "type": "module",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "pregen": "node -e \"require('fs').rmSync('src/generated', { recursive: true, force: true })\"",
    "gen-script": "bun run scripts/generateSchemas.ts",
    "gen:types": "bun run gen-script types",
    "gen:zod": "bun run gen-script zod",
    "gen:typebox": "bun run gen-script typebox",
    "gen:valibot": "bun run gen-script valibot",
    "gen:yup": "bun run gen-script yup",
    "gen:effect": "bun run gen-script effect",
    "gen:jsonschema": "bun run gen-script jsonschema",
    "prebuild:types": "node -e \"require('fs').rmSync('dist', { recursive: true, force: true })\"",
    "build:types": "tsc",
    "gen": "echo 'Running all schema generators...' && bun run gen:types && bun run gen:zod && bun run gen:typebox && bun run gen:valibot && bun run gen:yup && bun run gen:effect && bun run gen:jsonschema",
    "build": "bun run gen && bun run build:types",
    "prepublishOnly": "bun run build",
    "publish": "npm publish --access public",
    "release:patch": "npm version patch && bun run publish",
    "release:minor": "npm version minor && bun run publish",
    "release:major": "npm version major && bun run publish",
    "push-release": "git push && git push --tags"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.1.2",
    "@effect/schema": "^0.75.5",
    "@sinclair/typebox-codegen": "^0.11.1",
    "@types/bun": "^1.2.18",
    "@types/node": "^24.0.15",
    "bun-types": "^1.2.18",
    "effect": "^3.16.16",
    "fs-extra": "^11.3.0",
    "glob": "^11.0.3",
    "lefthook": "^1.12.2",
    "ts-morph": "^26.0.0",
    "ts-to-zod": "^3.15.0",
    "typebox": "^0.0.1",
    "typescript": "^5.8.3",
    "ultracite": "^5.0.46",
    "valibot": "^1.1.0",
    "yup": "^1.6.1",
    "zod": "^4.0.5"
  },
  "exports": {
    "./*": {
      "types": "./dist/types/*/index.d.ts",
      "import": "./dist/types/*/index.js",
      "require": "./dist/types/*/index.js"
    },
    "./*/details": {
      "types": "./dist/types/*/details.d.ts",
      "import": "./dist/types/*/details.js",
      "require": "./dist/types/*/details.js"
    },
    "./zod/*": {
      "types": "./dist/zod/*/index.d.ts",
      "import": "./dist/zod/*/index.js",
      "require": "./dist/zod/*/index.js"
    },
    "./zod/*/details": {
      "types": "./dist/zod/*/details.d.ts",
      "import": "./dist/zod/*/details.js",
      "require": "./dist/zod/*/details.js"
    },
    "./typebox/*": {
      "types": "./dist/typebox/*/index.d.ts",
      "import": "./dist/typebox/*/index.js",
      "require": "./dist/typebox/*/index.js"
    },
    "./typebox/*/details": {
      "types": "./dist/typebox/*/details.d.ts",
      "import": "./dist/typebox/*/details.js",
      "require": "./dist/typebox/*/details.js"
    },
    "./valibot/*": {
      "types": "./dist/valibot/*/index.d.ts",
      "import": "./dist/valibot/*/index.js",
      "require": "./dist/valibot/*/index.js"
    },
    "./valibot/*/details": {
      "types": "./dist/valibot/*/details.d.ts",
      "import": "./dist/valibot/*/details.js",
      "require": "./dist/valibot/*/details.js"
    },
    "./yup/*": {
      "types": "./dist/yup/*/index.d.ts",
      "import": "./dist/yup/*/index.js",
      "require": "./dist/yup/*/index.js"
    },
    "./yup/*/details": {
      "types": "./dist/yup/*/details.d.ts",
      "import": "./dist/yup/*/details.js",
      "require": "./dist/yup/*/details.js"
    },
    "./effect/*": {
      "types": "./dist/effect/*/index.d.ts",
      "import": "./dist/effect/*/index.js",
      "require": "./dist/effect/*/index.js"
    },
    "./effect/*/details": {
      "types": "./dist/effect/*/details.d.ts",
      "import": "./dist/effect/*/details.js",
      "require": "./dist/effect/*/details.js"
    },
    "./package.json": "./package.json"
  },
  "peerDependencies": {
    "@effect/schema": "^0.75.5",
    "effect": "^3.16.16",
    "typebox": "^0.0.1",
    "valibot": "^1.1.0",
    "yup": "^1.6.1",
    "zod": "^3.0.0"
  },
  "peerDependenciesMeta": {
    "zod": {
      "optional": true
    },
    "typebox": {
      "optional": true
    },
    "valibot": {
      "optional": true
    },
    "yup": {
      "optional": true
    },
    "effect": {
      "optional": true
    },
    "@effect/schema": {
      "optional": true
    }
  }
}